# POST    /user

GET     /user/:accountId    com.github.hottocoffee.controllers.UserController.find(accountId: String)
# PUT     /user/:id

# POST    /post

GET     /post/:postId       com.github.hottocoffee.controllers.PostController.find(postId: Int)
# DELETE  /post/:post_id

GET     /timeline           com.github.hottocoffee.controllers.TimelineController.list(lower_post_id: Int | Null, upper_post_id: Int | Null)
